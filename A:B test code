    "# Find duplicate user data\n",
    "df[df['user_id'].duplicated()]['user_id']  # the first column is index, and the second column is id\n",
    "# Save the user_id for all duplicate ids\n",
    "del_id = df[df['user_id'].duplicated()]['user_id'].values\n",
    "# Select those are not duplicate ids\n",
    "df_new = df[~df['user_id'].isin(del_id)]\n",
    "df_new"
