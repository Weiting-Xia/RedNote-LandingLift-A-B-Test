   "outputs": [
    {
     "data": {
      "text/plain": [
       "[np.int64(2031), np.int64(2031)]"
      ]
     },
     "execution_count": 148,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from statsmodels.stats.proportion import proportions_ztest, proportion_confint\n",
    "# Extract conversion results\n",
    "c_results = ab_test[ab_test['group'] == 'control']['converted']\n",
    "t_results = ab_test[ab_test['group'] == 'treatment']['converted']\n",
    "\n",
    "# Calculate the total experiment numbers\n",
    "n_c = c_results.count()\n",
    "n_t = t_results.count()\n",
    "# Calculate the number of successful convertion\n",
    "successes = [c_results.sum(), t_results.sum()]\n",
    "nobs = [n_c, n_t]\n",
    "nobs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 149,
   "id": "6717aae9",
   "metadata": {
    "papermill": {
     "duration": 0.029728,
     "end_time": "2023-09-28T10:01:27.795891",
     "exception": false,
     "start_time": "2023-09-28T10:01:27.766163",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "z_stat:0.48\n",
      "p:0.63\n",
      "95% for control:[0.108, 0.137]\n",
      "95% for treatment:[0.104, 0.132]\n"
     ]
    }
   ],
   "source": [
    "# Conduct Z-test\n",
    "# Calculate the estimator and p-value\n",
    "z_stat, pval = proportions_ztest(successes, nobs = nobs)\n",
    "# We choose the 95% confidence interval(alpha=0.05)\n",
    "(lower_c, lower_t), (upper_c, upper_t) = proportion_confint(successes, nobs = nobs, alpha=0.05)\n",
    "print(f'z_stat:{z_stat:.2f}')\n",
    "print(f'p:{pval:.2f}')\n",
    "print(f'95% for control:[{lower_c:.3f}, {upper_c:.3f}]')\n",
    "print(f'95% for treatment:[{lower_t:.3f}, {upper_t:.3f}]')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "df723e38-b8f6-4b4e-9ec6-200a2faeb774",
   "metadata": {},
   "outputs": [],
   "source": []
  },
